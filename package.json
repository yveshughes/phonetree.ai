{
  "name": "opaac-api",
  "version": "0.0.2",
  "description": "opaac module",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "scripts": {
    "start": "node dist/server.js",
    "tsc": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "publishX": "publish --access=public",
    "update-cybersafepolicy-core": "npm uninstall cybersafepolicy-core && npm install cybersafepolicy-core@latest",
    "postinstall": "npm run tsc",
    "dev-start": "npm run tsc && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yveshughes/opaac.git"
  },
  "keywords": [
    "opaac"
  ],
  "author": "Vinay Patidar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yveshughes/core/issues"
  },
  "homepage": "https://github.com/yveshughes/core#readme",
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.3",
    "@types/pg": "^8.6.1",
    "@types/uuid": "^8.3.3",
    "prettier": "^2.2.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsforce": "^1.9.33",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/lodash": "^4.14.178",
    "@types/multer": "^1.4.7",
    "@types/node-cron": "^3.0.0",
    "aws-sdk": "^2.1063.0",
    "bcrypt": "^5.0.1",
    "cybersafepolicy-core": "^0.0.71",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsforce": "^1.10.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "multer": "^1.4.4",
    "node-cron": "^3.0.0",
    "pg": "^8.6.0",
    "stripe": "^8.191.0",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": "16"
  }
}
